{
	"extends": "./tsconfig.base.json",
	"compilerOptions": {
		"baseUrl": ".",
		"types": ["vite/client"],
		"target": "ES2020",
		"module": "ES2020",
		"moduleResolution": "node",
		"importHelpers": false,
		"jsx": "preserve",
		"lib": ["ES2020", "DOM"],
		"noEmit": true
	},
	"exclude": ["src/server/**/*", "dist"],
	"typeAcquisition": { "enable": true }
}
